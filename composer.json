{
    "name": "php-documentor/html5",
    "type": "library",
    "description": "Html5-document specification for documentor",
    "license": "MIT",
    "keywords": ["specification", "html", "DOM"],
    "homepage": "https://github.com/php-documentor",
    "support": {
        "issues": "https://github.com/php-documentor/issues",
        "source": "https://github.com/php-documentor/html5"
    },
    "authors": [
        {
            "name": "Ivan Mezinov",
            "email": "ivanmezinov@mail.ru"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-mbstring": "*",
        "php-documentor/common-contracts": "dev-main",
        "php-documentor/specification-contracts": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "PhpDocumentor\\Html5\\": "src/"
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "PhpDocumentor\\Html5\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
